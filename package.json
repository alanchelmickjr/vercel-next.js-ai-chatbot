  "name": "myriad-chat",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "pnpm exec tsx lib/db/migrate && pnpm exec tsx lib/db/seed-models.ts && pnpm exec tsx lib/db/seed-prompts.ts && next build",
    "start": "next start",
    "lint": "next lint && biome lint --write --unsafe",
    "lint:fix": "next lint --fix && biome lint --write --unsafe",
    "format": "biome format --write",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "pnpm exec tsx lib/db/migrate.ts",
    "db:seed-models": "pnpm exec tsx lib/db/seed-models.ts",
    "db:seed-prompts": "pnpm exec tsx lib/db/seed-prompts.ts",
    "db:seed": "pnpm run db:seed-models && pnpm run db:seed-prompts",
    "db:setup": "pnpm run db:migrate && pnpm run db:seed",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "test": "export PLAYWRIGHT=True && pnpm exec playwright test --workers=4"
  },
